digraph Phy {
    rankdir = LR
    fontname = "Bitstream Vera Sans"
    fontsize = 8

    node [
        fontname = "Bitstream Vera Sans"
        fontsize = 8
        shape = "record"
        style = "filled"
        fillcolor="lightblue3"
    ]

    edge [
        fontname = "Bitstream Vera Sans"
        fontsize = 8
    ]
    
    Character [
        label = "character|\
      PK character_id INT\l\
         character_name String(100)\l"
    ]
    
    Warrior [
        label = "warrior|\
      PK warrior_id INT\l\
     FK+ character_id INT NOT NULL\l\
         warrior_weapon String(100)\l"
    ]
    
    Item [\
        label = "item|\
      PK item_id INT\l\
      FK character_id INT NOT NULL\l\
         item_name String(100)\l"
    ]
    
    Group [
        label = "group|\
      PK group_id INT\l\
         group_name String(100)\l"
    ]
    
    Pet [
        label = "pet|\
      PK pet_id INT\l\
      FK character_id INT\l\
         pet_name\l"
    ]
    
    UserGroup [
        label = "user_group|\
      PK user_group_id INT\l\
      FK user_id INT NOT NULL\l\
      FK group_id INT NOT NULL\l"
    ]

    Character -> Item
    Character -> Pet
    Character -> Warrior
    Character -> UserGroup
    Group -> UserGroup
   
}
